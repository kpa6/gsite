type="filepicker"
data-fp-drag-text="or drop image here"
data-fp-extensions=".png,.jpeg,.gif"
data-fp-button-class="btn file-upload"
data-fp-multiple="false"
data-fp-store-location="S3"
data-fp-store-path="/{{=it.site.domain}}/"
data-fp-store-access="public"
onchange="addPicToPreview(this, event, 'http://{{=it.env.AWS_CLOUDFRONT_STATIC}}/' + event.fpfile.key);"
